@model IEnumerable<SkemaSystem.Models.Room>

@{
    ViewBag.Title = "Lokaler";
}

@section Subheader {
    @Html.Partial("_AdminTabsPartial")
}

<p>
    @Html.ActionLink("Opret ny", "Create")
</p>
<table class="table">
    <tr>
        <th>
           Lokale navn
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RoomName)
            </td>
            <td>
                @Html.ActionLink("Ændre", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Detaljer", "Details", new { id = item.Id }) |
                @Html.ActionLink("Slet", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>